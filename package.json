{
    "name": "my-app",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && npm run package",
        "preview": "vite preview",
        "package": "svelte-kit sync && svelte-package && publint",
        "prepublishOnly": "npm run package",
        "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        }
    },
    "peerDependencies": {
        "svelte": "^5.0.0"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.3.1",
        "@sveltejs/kit": "^2.20.4",
        "@sveltejs/package": "^2.3.7",
        "@sveltejs/vite-plugin-svelte": "^5.0.2",
        "@types/leaflet": "^1.9.15",
        "publint": "^0.2.12",
        "svelte": "^5.12.0",
        "svelte-check": "^4.1.1",
        "typescript": "^5.7.2",
        "vite": "^6.0.3"
    },
    "dependencies": {
        "20": "^3.1.9",
        "@supabase/auth-helpers-sveltekit": "^0.13.0",
        "@supabase/supabase-js": "^2.49.4",
        "face-api.js": "^0.22.2",
        "leaflet": "^1.9.4",
        "leaflet-routing-machine": "^3.2.12",
        "svelte-webcam": "^0.0.0",
        "tesseract.js": "^6.0.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "^4.28.1"
    },
    "description": "Everything you need to build a Svelte library, powered by [`create-svelte`](https://github.com/sveltejs/kit/tree/main/packages/create-svelte).",
    "main": "svelte.config.js",
    "keywords": [],
    "author": "",
    "license": "ISC"
}
